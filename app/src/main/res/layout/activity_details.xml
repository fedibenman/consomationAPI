<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <TextView

        android:id="@+id/code_affichage"
        android:layout_width="wrap_content"
        android:layout_height="20dp">
    </TextView>
    <TextView

        android:id="@+id/intitule_affichage"
        android:layout_width="wrap_content"
        android:layout_height="20dp">
    </TextView>
    <TextView

        android:id="@+id/societe_affichage"
        android:layout_width="wrap_content"
        android:layout_height="20dp">
    </TextView>
    <TextView
        android:id="@+id/specialite_affichage"
        android:layout_width="wrap_content"
        android:layout_height="20dp">
    </TextView>
    <TextView
        android:id="@+id/nbspots_affichage"
        android:layout_width="wrap_content"
        android:layout_height="20dp">
    </TextView>
    <TextView
        android:id="@+id/pays_affichage"
        android:layout_width="wrap_content"
        android:layout_height="20dp">
    </TextView>
</LinearLayout>